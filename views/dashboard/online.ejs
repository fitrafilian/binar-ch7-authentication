<main>
  <!-- Page content-->
  <div class="container-fluid">
    <h1 class="mt-4">Welcome to Dashboard</h1>
    <div class="list-group">
      <% users.forEach(user => { %>
      <li class="list-group-item">
        <div class="container">
          <div class="row">
            <div class="col-3 col-md-1 col-lg-1">
              <img style="width: 40px; height: 40px" src="image/profile.svg" alt="" />
            </div>
            <div class="col">
              <h5 class="mb-0"><%= user.userdata.firstName + ' ' + user.userdata.lastName %></h5>
              <p class="mb-0"><%= user.userdata.email %></p>
            </div>
          </div>
        </div>
      </li>
      <% }) %>
    </div>
  </div>
</main>
